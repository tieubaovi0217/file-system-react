ame) => {\r\n    // const updatedPath = path === '' ? name : `${path}/${name}`;\r\n    // console.log(updatedPath);\r\n    dispatch(fetchFileBrowserDataAsync(`${path}/${name}`));\r\n  };\r\n\r\n  const handleDownload = (name) => {};\r\n\r\n  const files = items\r\n    .filter(\r\n      (item) =>\r\n        item.type === 'file' && item.name.startsWith(filterValue.trim()),\r\n    )\r\n    .map((file) => (\r\n      <File\r\n        name={file.name}\r\n        mtime={file.mtime}\r\n        size={file.size}\r\n        onDelete={handleDeleteFileOrFolder}\r\n      />\r\n    ));\r\n\r\n  const folders = items\r\n    .filter(\r\n      (item) =>\r\n        item.type === 'directory' && item.name.startsWith(filterValue.trim()),\r\n    )\r\n    .map((folder) => (\r\n      <Folder\r\n        path={path}\r\n        mtime={folder.mtime}\r\n        name={folder.name}\r\n        size={folder.size}\r\n        onDelete={handleDeleteFileOrFolder}\r\n        onDoubleClick={handleFolderDoubleClick}\r\n      />\r\n    ));\r\n\r\n  return (\r\n    <Layout>\r\n      <Header style={{ borderBottom: '0px' }}>\r\n        <FileBrowserHeader\r\n          currentPath={path}\r\n          onSearchChange={handleSearchChange}\r\n          onBackButtonClick={handleBackButtonClick}\r\n        />\r\n      </Header>\r\n      <Content>\r\n        <div className=\"file-browser__content\">\r\n          <Row gutter={[8, 8]}>\r\n            {folders}\r\n            {files}\r\n          </Row>\r\n        </div>\r\n      </Content>\r\n    </Layout>\r\n  );\r\n};\r\n\r\nexport default FileBrowserContent;\r\n","C:\\Users\\fpt\\MyThesis\\file-system-reactjs\\src\\components\\FileBrowser\\FileBrowserHeader\\index.js",[],"C:\\Users\\fpt\\MyThesis\\file-system-reactjs\\src\\components\\FileBrowser\\UploadFolder\\index.js",[],"C:\\Users\\fpt\\MyThesis\\file-system-reactjs\\src\\components\\FileBrowser\\UploadFile\\index.js",[],"C:\\Users\\fpt\\MyThesis\\file-system-reactjs\\src\\components\\FileBrowser\\Folder\\index.js